package org.all;
import org.tamil.Tamil;
import org.english.English;
import org.telgu.Telugu;
public class Languageclass {
	
	
	public void allLanguage() {
		System.out.println("All languages are Tamil,Telugu and English");
		
	}

	public static void main(String[] args) {
		Languageclass W = new Languageclass();
		W.allLanguage();
		}

	private void tamilLanguage() {
		Tamil X= new Tamil();
		X.tamilLanguage();
	}

	private void englishLanguage() {
		English Y = new English();
		Y.englishLanguage();
		
		
	}

	private void telguLanguage() {
Telugu Z = new Telugu();
		Z.telguLanguage();
	}
	

}
