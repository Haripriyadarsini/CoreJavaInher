package org.hierac;

public class Animal {

	public void Zoo() {
		System.out.println("Lion,Tiger,Monkey");
	}

public class PetAnimal extends Animal {
	
	public void house() {
		System.out.println("Cat,Dog,Hen");
	}
}

public class Wildanimal extends Animal {
	
	public void Forest() {
		System.out.println("Elephant,Zebra,Hippo");
	}
}

